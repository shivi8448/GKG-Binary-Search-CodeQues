class Solution {
    int binarysearch(int arr[], int n, int k) {
        int i=0;
        int flag=0;
        for(i=0;i<n;i++)
        {
            if(arr[i]==k)
            {
            flag=1;
            break;
        }
        }
        if(flag==1)
        {
        return i;
        }
        
        else
        {
        return -1;
        }
    }
}
